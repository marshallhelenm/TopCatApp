<div id="garden">

<div id="catpic">
Cat pic?
</div>
<div id="new_form">
<%= form_for @cat do |c| %>

    <%= c.label :name %><br>
    <%= c.text_field :name %><br>
    
    <%= c.label :breed %><br>
    <%= c.text_field :breed %><br>
    
    <%= c.label :description %><br>
    <%= c.text_area :description %><br>

    <%= c.submit %>
<% end %>
</div>
</div>